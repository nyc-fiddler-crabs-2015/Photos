<h2><%= @category.title %></h2>

<a href="/categories/<%= @category.id %>/photos/uploader">Add Photo</a>

<div>
  <% @photos.each do |photo| %>
    <p>
      <a href="/photos/<%= photo.id %>">
        <img src="<%= photo.photo_url %>">
      </a>
    </p>
  <% end %>
</div>







<a href="/categories">Back</a>
